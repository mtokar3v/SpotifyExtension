{
	"manifest_version": 2,
	"name": "SpotifyExtension",
	"description": "Extension for spotify",
	"version": "1.0.0",
	"author": "Tokarev Maxim",
	"browser_action": {
		//"default_popup": "index.html"
	},
	"content_scripts": [
		{
			"matches": ["*://open.spotify.com/*"],
			"js": ["content.js"]
		}
	],
	"permissions": ["activeTab"]
}